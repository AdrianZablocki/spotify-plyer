{
  "name": "client",
  "version": "1.0.0",
  "description": "Spotify player - client application for quick review of user lists",
  "scripts": {
    "start": "webpack-dev-server -r dotenv/config --port 3010 --config webpack/webpack.development.js --open --history-api-fallback",
    "build": "npx rimraf ./build && webpack -r dotenv/config --display-error-details --config webpack/webpack.production.js",
    "test": "jest --coverage --updateSnapshot --json --outputFile=./reports/unit-test-report.json --passWithNoTests",
    "test:watch": "jest --watch --runInBand",
    "eslint": "eslint --ignore-path .gitignore ./ --ext .js,.ts,.tsx",
    "eslint:fix": "npm run eslint --fix",
    "eslint:json": "npm run eslint --quiet -f json -o reports/eslint.json",
    "stylelint": "stylelint \"**/*.(ts|tsx)\" --ignore-path .gitignore --allow-empty-input",
    "stylelint:fix": "npm run stylelint --fix",
    "stylelint:json": "npm run stylelint --formatter json --output-file reports/stylelint.json",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|scss|yml|json)\"",
    "format": "npm run prettier --write",
    "find-duplicates": "npx jscpd ./src/**",
    "pre-commit": "lint-staged"
  },
  "keywords": [],
  "author": "AppSynth",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "7.10.4",
    "axios": "0.19.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "spotify-web-api-js": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/plugin-transform-runtime": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.9.0",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "7.6.0",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.3",
    "@types/jest": "25.2.1",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-emotion": "10.0.33",
    "babel-plugin-module-resolver": "4.0.0",
    "browserslist-useragent-regexp": "2.1.0",
    "copy-webpack-plugin": "6.0.3",
    "dotenv-webpack": "1.8.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "6.0.0",
    "handlebars": "4.7.6",
    "handlebars-loader": "1.7.1",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.1.0",
    "jest-junit": "11.0.1",
    "jscpd": "3.2.1",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-prettier": "1.1.2",
    "typescript": "3.8.3",
    "webpack": "5.0.0-beta.22",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "2.5.0"
  }
}
